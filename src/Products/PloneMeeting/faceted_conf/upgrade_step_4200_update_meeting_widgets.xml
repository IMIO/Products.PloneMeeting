  </criterion>

  <criterion purge="True" name="c5" i18n:attributes="title">
   <property name="widget">daterange</property>
   <property name="title">Date</property>
   <property name="position">top</property>
   <property name="section">advanced</property>
   <property name="hidden">False</property>
   <property name="index">meeting_date</property>
   <property name="default"></property>
   <property name="calYearRange">c-30:c+2</property>
   <property name="labelStart"></property>
   <property name="labelEnd"></property>
   <property name="placeholderStart">Start date</property>
   <property name="placeholderEnd">End date</property>
