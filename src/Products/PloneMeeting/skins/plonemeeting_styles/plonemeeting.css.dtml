/* PloneMeeting implementation of a selection box whose elements are icons + texts */
.ploneMeetingSelectContainer{
    float: left;
}
#content .ploneMeetingSelectContainer p {
    margin-right: 1em;
}
.ploneMeetingSelectButton {
    cursor: pointer;
    border: 1px solid &dtml-globalBorderColor;;
    background-color: white;
    padding: 0px;
    color: &dtml-fontColor;;
    text-align: left;
    width: auto;
    height: 18px !important;
    padding-right: 24px;
    margin-top: 5px;
}
.ploneMeetingSelectBox {
    cursor: pointer;
    border-width: 1px;
    border-color: &dtml-globalBorderColor;;
    background-color: white;
    padding: 0px;
    color: &dtml-fontColor;;
    display: none;
    position: absolute;
    border-style: inset;
    height: auto;
    overflow: auto;
    text-align: left;
    width: auto;
    min-width: 274px;
    z-index: 1;
}
.ploneMeetingSelectItem {
    margin: 0px 1px 0px 1px;
    width: 99%; /* If 100% a horizontal scrollbar is displayed */
    vertical-align: top !important;
    min-height: 18px;
}
#content p.ploneMeetingSelectItem {
    margin-bottom: 0em;
}
/* add some padding before a annex sub type */
#content p.ploneMeetingSelectItem.subtype img {
    margin-left: 15px;
}

.ploneMeetingSelectItemUnselected {
    background-color: &dtml-globalBackgroundColor;;
}
.ploneMeetingSelectImage {
    max-height: 16px;
    max-width: 16px;
    vertical-align: top;
}
.ploneMeetingSelectDropdownIcon {
    height: 16px;
    background-image: url(&dtml-portal_url;/drop_down.png);
    background-repeat: no-repeat;
    background-position: right;
    min-width: 250px;
}

/* while copy/pasting a table from Word in CKeditor
   there are no border...  So force border of table displayed in item rich fields.
   .cke_show_borders is an override for CKeditor while displaying tables inside the editor. */
#pmDescription table td, #pmDescription table tr,
[id*="hook_"] fieldset table tr, [id*="hook_"] fieldset table td,
.cke_show_borders table.cke_show_border,
.cke_show_borders table.cke_show_border > tr > td,
.cke_show_borders table.cke_show_border > tr > th,
.cke_show_borders table.cke_show_border > tbody > tr > td,
.cke_show_borders table.cke_show_border > tbody > tr > th,
.cke_show_borders table.cke_show_border > thead > tr > td,
.cke_show_borders table.cke_show_border > thead > tr > th,
.cke_show_borders table.cke_show_border > tfoot > tr > td,
.cke_show_borders table.cke_show_border > tfoot > tr > th,
.cke_editable table td {
    border: 1px solid black !important;
}

/* make sure table borders in pmMoreInfo (when using glasses icon) are displayed */
#content span.pmMoreInfo td {
    border: 1px solid black;
}

/* used for extra infos displayed on items listings or on the meeting view when using 'glasses icon action' */
