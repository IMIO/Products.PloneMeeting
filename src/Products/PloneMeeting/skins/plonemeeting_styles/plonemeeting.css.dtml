}

/* display input for entering TAL expression as large as possible */
input#form-widgets-as_copy_group_on {
    width: 99%;
}
/* when hovering [auto] on item view */
strong.auto_info {
    cursor: help;
}

.navigateItem {
    text-align: center;
    clear: both;
    margin-top: 0.5em;
}
.navigateItem img {
    vertical-align: baseline;
}

#content div#plone-document-byline {
  float: left;
}
div.viewlet_workflowstate {
    float: right;
}

/* make descripiton display correctly as document byline is floating */
div#content .documentDescription, #content #description {
    padding-bottom: 0em;
    padding-top: 1em;
}

/* Colors on states */
body:not([class*='section-contacts']) .state-active,
.label-state-active {
   color: green !important;
}
.state-inactive,
.state-inactive a,
.label-state-inactive,
.state-deactivated {
    color: red !important;
}

/* red */
div.viewlet_workflowstate span.state-created,
div.viewlet_workflowstate span.state-inactive,
div.viewlet_workflowstate span.state-itemcreated,
div.viewlet_workflowstate span.state-itemcreated_waiting_advices,
div.viewlet_workflowstate span.state-advice_under_edit {
    background-color: red;
    border-color: red;
}
.state-created,
.state-itemcreated,
#content .pretty_link_content.state-itemcreated_waiting_advices,
.state-to_hidden_during_redaction_action,
.state-advice_under_edit {color: red !important;}
/* DarkOrange, special management for itemcreated_waiting_advices that is red */
div.viewlet_workflowstate span[class^='state-proposed'],
div.viewlet_workflowstate span[class^='state-returned_to_proposing_group_proposed'],
div.viewlet_workflowstate span[class$='waiting_advices']:not([class='state-itemcreated_waiting_advices']),
div.viewlet_workflowstate span.state-prevalidated,
div.viewlet_workflowstate span.state-returned_to_proposing_group_prevalidated {
    background-color: DarkOrange;
    border-color: DarkOrange;
}
[class*='state-proposed'],
[class*='state-returned_to_proposing_group_proposed'],
[class*='state-'][class$='waiting_advices'],
.state-prevalidated,
.state-returned_to_proposing_group_prevalidated {
   color: DarkOrange !important;
}
div.viewlet_workflowstate span.state-validated {
    background-color: blue;
    border-color: blue;
}
.state-validated {
   color: blue !important;
}
div.viewlet_workflowstate span.state-presented {
    background-color: brown;
    border-color: brown;
}
.state-presented {
   color: brown !important;
}
/* cornflowerBlue */
div.viewlet_workflowstate span.state-itemfrozen,
div.viewlet_workflowstate span.state-frozen {
    background-color: cornflowerBlue;
    border-color: cornflowerBlue;
}
.state-itemfrozen,
.state-frozen {
   color: cornflowerBlue !important;
}
/* grey */
div.viewlet_workflowstate span.state-returned_to_proposing_group,
div.viewlet_workflowstate span.state-closed,
div.viewlet_workflowstate span.state-delayed,
div.viewlet_workflowstate span.state-marked_not_applicable,
div.viewlet_workflowstate span.state-postponed_next_meeting,
div.viewlet_workflowstate span.state-removed,
div.viewlet_workflowstate span.state-advice_given {
    background-color: grey;
    border-color: grey;
}
.state-returned_to_proposing_group,
.state-closed,
.state-delayed,
.state-marked_not_applicable,
.state-postponed_next_meeting,
.state-removed,
.state-advice_given {color: grey !important;}
/* darkgoldenrod */
div.viewlet_workflowstate span.state-itempublished,
div.viewlet_workflowstate span.state-published {
    background-color: darkgoldenrod;
    border-color: darkgoldenrod;
}
.state-itempublished,
.state-published {color: darkgoldenrod !important;}
/* black */
div.viewlet_workflowstate span.state-decided,
div.viewlet_workflowstate span.state-itemdecided,
div.viewlet_workflowstate span.state-decisions_published {
    background-color: black;
    border-color: black;
}
.state-decided,
.state-itemdecided,
.state-decisions_published {
   color: black !important;
}
div.viewlet_workflowstate span.state-active,
div.viewlet_workflowstate span.state-accepted,
div.viewlet_workflowstate span.state-accepted_but_modified,
div.viewlet_workflowstate span.state-accepted_out_of_meeting,
div.viewlet_workflowstate span.state-accepted_out_of_meeting_emergency,
div.viewlet_workflowstate span.state-pre_accepted {
    background-color: green;
    border-color: green;
}
.state-accepted,
.state-accepted_but_modified,
.state-accepted_out_of_meeting,
.state-accepted_out_of_meeting_emergency,
.state-to_not_hidden_during_redaction_action,
.state-pre_accepted {
   color: green !important;
}
div.viewlet_workflowstate span.state-refused {
    background-color: violet;
    border-color: violet;
}
.state-refused {
   color: violet !important;
}
div.viewlet_workflowstate span.state-transfered {
    background-color: #6471A6;
    border-color: #6471A6;
}
.state-transfered {
   color: #6471A6 !important;
}

/* Colors on transitions, used in the item wf history */
/* this correspond to the element creation */
.transition-none { color: red !important; }
.transition-activate {}
.transition-deactivate {color: red !important; }
.transition-propose { color: #ff6c00 !important; }
.transition-validate { color: blue !important; }
.transition-present { color: brown !important; }
.transition-itemfreeze,
.transition-freeze { color: cornFlowerBlue !important; }
.transition-return_to_meeting,
.transition-return_to_proposing_group { color: grey !important; }
.transition-itempublish, .transition-publish { color: darkgoldenrod !important; }
.transition-accept { color: #64c168 !important; }
.transition-confirm { color: green !important; }
.transition-delay { color: grey !important; }
.transition-refuse { color: violet !important; }
.transition-close { color: black !important; }

