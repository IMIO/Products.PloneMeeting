}
#content div#labeling-viewlet {
   padding-top: 0.3em;
}
/* labels displayed in the dashboards */
#content div#faceted-results div#labeling-viewlet ul {
   display: inline-block;
}

/* grouped configs displayed in tooltipster */
div.grouped-configs {
    padding-right: 1em;
    padding-left: 1em;
}

/* arrow position when tooltipster displayed to top (annexes at bottom of the screen) */
.tooltipster-sidetip.tooltipster-top .tooltipster-arrow {
    height: 6px;
}

#content span.help-doc {
   border-bottom: 1px dotted;
   padding-bottom: 2px;
   cursor: pointer;
}

/* meeting_users table on meeting_edit */
table#meeting_users td {
   padding: 0.5em;
}
table#meeting_users td.draggable {
   cursor: ns-resize;
}
table#meeting_users select[id$="_replacement"] {
   width: 500px;
}
table.listing tbody tr.odd.selected td,
table.listing tbody tr.odd.dragging td,
table.listing tbody tr.even.selected td,
table.listing tbody tr.even.dragging td {
    background-color: #ffa;
}

/* check in item people tables */
#content span.people-checked:before,
#content span.people-signatory-checked:before {
    content: "\f00c";
}
#content span.people-edit-itemabsent:before,
#content span.people-left:before {
    content: "\f2f5";
    font-size: 120%;
}
#content span.people-left:before {
    color: red;
    cursor: help;
}
#content span.people-edit-itemabsent:before,
#content span.people-edit-signatory:before {
    color: orange;
}
#content span.people-action {
    border-bottom: 1px dotted;
}

#content span.people-edit-signatory:before {
    content: "\f5b7";
    font-size: 120%;
}
#content span.people-signatory-replacer:before,
#content span.people-not-present:before {
    content: "\f00c";
    font-size: 120%;
    color: red;
    cursor: help;
}
.item-signatory-replacer-signature-number {
    font-family: &dtml-fontFamily;;
    font-weight: normal;
}
.tooltipster-meeting-item-signatories {
   cursor: help;
   color: red;
}

#content span.pmHelp {
   cursor: help;
}

span.signatory-checked,
span.signatory-replaced,
span.signatory-replacer {
   cursor: help;
}
span.signatory-replaced,
span.signatory-replacer {
   color: red;
}

#content span.people-remove-left:before,
#content span.people-remove-redefined-signatory:before {
    content: "\f00d";
    color: red;
    font-size: 120%;
}

div.item-send-to-other-mc {
   display: inline-block;
}
div.item-send-to-other-mc-multiple-fields {
   border: 1px solid lightgrey;
   min-width: 600px;
}

/* hide downloard vcard link on contacts */
p#download_vcard {
   display: none;
}

/* input to enter item number to go to on the item widget navigation */
#content input.goto_meeting {
    vertical-align: baseline;
}

.collapsible-content {
    /*max-height: 0;*/
    overflow: hidden;
    transition: max-height 0.2s ease-out;
    margin-bottom: 0.5em;
}
.collapsible-inner-content {
    margin: 0.2em;
    padding: 0.3em;
    background-color: &dtml-evenRowBackgroundColor;;
    border:1px solid &dtml-tableHeaderBackgroundColor;;
}
.collapsible-content.collapsed-on-load {
    max-height: inherit;
}

.collapsible {
    background-color: transparent;
    cursor: pointer;
    border: none;
    text-align: left;
    outline: none;
}

.active, .collapsible:hover {
    background-color: transparent;
}

.collapsible:before {
    font-family: "Font Awesome 5 Free";
    content: "\f0fe";
    color: grey;
    font-weight: bold;
    margin-left: 2px;
}

.active:before {
    font-family: "Font Awesome 5 Free";
    content: "\f146";
}

/* hide app_parameters for plonegroup root organization */
body:not([class*='subsection-plonegroup-organization-']).template-view div:not([class^='tooltip']) fieldset#form-groups-app_parameters,
body:not([class*='subsection-plonegroup-organization-']).template-edit a#fieldsetlegend-app_parameters {
   display: none;
}

div.organization_level:not([class*='organization_level_0']),
div.config_sub_categories {
    margin-left: 1.5em;
    margin-top: 0.5em;
}
div[class*='organization_level_']:not([class*='organization_level_0']) table,
div.config_sub_categories table {
   border-left: 5px solid grey;
}

/* highlight organizations not selectable in plonegroup */
div.highlight.selectable_for_plonegroup_false label,
div.highlight.selectable_for_plonegroup_false div span {
   color: red;
}

/* highlight disabled values in red in faceted filters */
#faceted-form fieldset.widget-fieldset label[title*="(Désactivé)"] {
    color: red;
}

/* increase FontAwesome base size when parent is using discreet */
.discreet .fa, .fas {
    font-size: 110%;
}

h2.available-items,
h2.presented-items,
h2.pm-inserting-methods-helper-msg {
   display: inline;
}

/* avoid select input to take to much place when editing certified signatures datagridfield */
div#formfield-form-widgets-certified_signatures th {
   width: 17%;   
}
div#formfield-form-widgets-certified_signatures td select.select-widget {
   width: 100%;   
}

a.portlet_pm_own_org_link {
   margin-top: 1em;
   font-weight: bold;
}

