#content fieldset {
   line-height: 1em;
   margin: 0.5em 0;
}

/* make padding/margin under label in overlays work */
div.overlay label {
    display: inline-block;
}

/* background of secret items when displayed on the meeting */
table.listing tbody tr.odd.meeting_item_privacy_secret td {
    background-color: #fffacd;
}
table.listing tbody tr.even.meeting_item_privacy_secret td {
    background-color: #ffe37f;
}
table.listing tbody tr.odd.meeting_item_privacy_secret_heading td {
    background-color: #d0ece7;
}
table.listing tbody tr.even.meeting_item_privacy_secret_heading td {
    background-color: #a2d9ce;
}
table.listing tbody tr.odd.meeting_item_privacy_public_heading td {
    background-color: #FFEDDE;
}
table.listing tbody tr.even.meeting_item_privacy_public_heading td {
    background-color: #FFDABC;
}

img.logical_meeting {
    cursor: help;
}

/* display lines of the table on the meeting_view 100% height so
   the 'listType' column is displayed correctly */
table.faceted-table-results > tbody > tr {
    height: 1px;
}

table.listing tr[class][class] td[class^='meeting_item_listType_'] {
    padding: 0em;
    border-right: 0px;
    height: 100%;
}

/* only for Chrome */
body.using-chrome table.listing tr[class][class] td[class^='meeting_item_listType_'] {
   height: inherit;
}

/* grey the items with a subnumber itemnumber input */
td input.subnumber,
td span.subnumber {
    background-color: lightgray;
    border-radius: 5px;
}
td span.subnumber {
    padding-left: 0.5em;
    padding-right: 0.5em;
}
table.listing tr.odd td.itemnumber,
table.listing tr.even td.itemnumber {
    text-align: center;
    padding-left: 0em;
    padding-right: 0.3em;
}

/* show div of the ColorColumn for other listTypes */
td[class^='meeting_item_listType_'] > div {
    height: 100%;
    width: 100%;
    cursor: help;
}

/* PloneMeeting implementation of a selection box whose elements are icons + texts */
dl.portlet .ploneMeetingSelectContainer p {
    margin-right: 1em;
}

dl.portlet .ploneMeetingSelectButton {
    cursor: pointer;
    border: 1px solid #d3d3d3;
    background-color: white;
    color: &dtml-fontColor;;
    text-align: left;
    width: auto;
    height: 18px !important;
    padding-right: 24px;
    padding-top: 3px;
    margin-top: 4px;
}
/* selected element */
dl.portlet .ploneMeetingSelectButton span.ploneMeetingRef,
dl.portlet p.ploneMeetingSelectItem.selected {
    font-weight: bold;
}

dl.portlet div.ploneMeetingSelectBox {
    cursor: pointer;
    border: 1px solid #d3d3d3;
    border-top: 0px;
    background-color: white;
    color: &dtml-fontColor;;
    display: none;
    position: absolute;
    padding-top: 0px;
    height: auto;
    overflow: auto;
    text-align: left;
    width: 95%;
    z-index: 1;
    overflow: hidden;
}
dl.portlet div.ploneMeetingSelectBox p span.select_choice {
    margin-top: 3px;
    display: inline-block;
    margin-bottom: 4px;
}
dl.portlet p.ploneMeetingSelectItem {
    margin: 0px;
    vertical-align: top !important;
    min-height: 18px;
}
dl.portlet p.ploneMeetingSelectItem a {
    min-height: 18px;
    padding-left: 5px;
    padding-top: 5px;
}
dl.portlet p.ploneMeetingSelectItem:hover,
dl.portlet p.ploneMeetingSelectItem:hover span {
    background-color: &dtml-tableHeaderBackgroundColor;;
    font-weight: bold;
}

dl.portlet .ploneMeetingSelectImage {
    max-height: 16px;
    max-width: 16px;
    vertical-align: top;
}
dl.portlet .ploneMeetingSelectDropdownIcon {
    height: 16px;
    background-image: url(&dtml-portal_url;/drop_down.png);
    background-repeat: no-repeat;
    background-position: right;
    padding-left: 5px;
}

/* show table using border="0" or border:none
   we use !important so even if border are defined on cells, it is displayed
   as dotted grey because appy.pod will not render it neither */
#content div[id^='hook_'] table[border='0'] tr th,
#content div[id^='hook_'] table[border='0'] tr td,
#content div[id^='hook_'] table[style*='border: none'] tr th,
#content div[id^='hook_'] table[style*='border: none'] tr td,
#content div[id^='hook_'] table[style*='border:none'] tr th,
#content div[id^='hook_'] table[style*='border:none'] tr td,
#content div[id^='hook_'] table[style*='border: undefined'] tr th,
#content div[id^='hook_'] table[style*='border: undefined'] tr td,
#content div[id^='hook_'] table[style*='border:undefined'] tr th,
#content div[id^='hook_'] table[style*='border:undefined'] tr td,
#content div[id^='hook_'] table[style*='border: medium none'] tr th,
#content div[id^='hook_'] table[style*='border: medium none'] tr td,
#content div[id^='hook_'] table[style*='border:medium none'] tr th,
#content div[id^='hook_'] table[style*='border:medium none'] tr td,
#content div[id^='hook_'] table[style*='border-collapse:collapse; border:none'] tr th,
#content div[id^='hook_'] table[style*='border-collapse:collapse; border:none'] tr td,
#content div[id^='hook_'] table[style*='border-bottom:none'] tr th,
#content div[id^='hook_'] table[style*='border-bottom:none'] tr td,
/* same rules as above must be applied when editing thru CKeditor */
body.cke_editable.cke_show_borders table[border='0'] tr th,
body.cke_editable.cke_show_borders table[border='0'] tr td,
body.cke_editable.cke_show_borders table[style*='border: none'] tr th,
body.cke_editable.cke_show_borders table[style*='border: none'] tr td,
body.cke_editable.cke_show_borders table[style*='border:none'] tr th,
body.cke_editable.cke_show_borders table[style*='border:none'] tr td,
body.cke_editable.cke_show_borders table[style*='border-collapse:collapse; border:none'] tr th,
body.cke_editable.cke_show_borders table[style*='border-collapse:collapse; border:none'] tr td,
/* same rules as above must be applied when displayed in dashboard statically */
#content div.static-infos table[border='0'] tr th,
#content div.static-infos table[border='0'] tr td,
#content div.static-infos table[style*='border: none'] tr th,
#content div.static-infos table[style*='border: none'] tr td,
#content div.static-infos table[style*='border:none'] tr th,
#content div.static-infos table[style*='border:none'] tr td,
#content div.static-infos table[style*='border: undefined'] tr th,
#content div.static-infos table[style*='border: undefined'] tr td,
#content div.static-infos table[style*='border:undefined'] tr th,
#content div.static-infos table[style*='border:undefined'] tr td,
#content div.static-infos table[style*='border: medium none'] tr th,
#content div.static-infos table[style*='border: medium none'] tr td,
#content div.static-infos table[style*='border:medium none'] tr th,
#content div.static-infos table[style*='border:medium none'] tr td,
#content div.static-infos table[style*='border-collapse:collapse; border:none'] tr th,
#content div.static-infos table[style*='border-collapse:collapse; border:none'] tr td,
#content div.static-infos table[style*='border-bottom:none'] tr th,
#content div.static-infos table[style*='border-bottom:none'] tr td,
/* same rules as above must be applied when displayed in dashboard thru show/hide details */
#content div.pmMoreInfo table[border='0'] tr th,
#content div.pmMoreInfo table[border='0'] tr td,
#content div.pmMoreInfo table[style*='border: none'] tr th,
#content div.pmMoreInfo table[style*='border: none'] tr td,
#content div.pmMoreInfo table[style*='border:none'] tr th,
#content div.pmMoreInfo table[style*='border:none'] tr td,
#content div.pmMoreInfo table[style*='border: undefined'] tr th,
#content div.pmMoreInfo table[style*='border: undefined'] tr td,
#content div.pmMoreInfo table[style*='border:undefined'] tr th,
#content div.pmMoreInfo table[style*='border:undefined'] tr td,
#content div.pmMoreInfo table[style*='border: medium none'] tr th,
#content div.pmMoreInfo table[style*='border: medium none'] tr td,
#content div.pmMoreInfo table[style*='border:medium none'] tr th,
#content div.pmMoreInfo table[style*='border:medium none'] tr td,
#content div.pmMoreInfo table[style*='border-collapse:collapse; border:none'] tr th,
#content div.pmMoreInfo table[style*='border-collapse:collapse; border:none'] tr td,
#content div.pmMoreInfo table[style*='border-bottom:none'] tr th,
#content div.pmMoreInfo table[style*='border-bottom:none'] tr td {
    border: 1px dotted black !important;
}
/* table for which attribute "border" is not given will be rendered with borders by appy.pod */
/* make sure table borders in pmMoreInfo (when using the "more infos" icon) are displayed */
/* make sure nested tables under a table using no-style-table displays border */
#content div[id^='hook_'] table:not([border]):not([style*="border"]) tr th,
#content div[id^='hook_'] table:not([border]):not([style*="border"]) tr td,
#content div.static-infos table:not([border]):not([style*="border"])  tr th,
#content div.static-infos table:not([border]):not([style*="border"])  tr td,
#content div.pmMoreInfo table:not([border]):not([style*="border"])  tr th,
#content div.pmMoreInfo table:not([border]):not([style*="border"])  tr td,
body.cke_editable table:not([border='0']):not([style*="border"])  tr td,
body.cke_editable table.cke_show_border:not([border='0']):not([style*="border"])  tr td,
#content table.no-style-table table:not([class='listing datagridwidget-table-view']) tr td,
#content div.static-infos table[border='1'] tr th,
#content div.static-infos table[border='1'] tr td,
#content div.pmMoreInfo table[border='1'] tr th,
#content div.pmMoreInfo table[border='1'] tr td {
    border: 1px solid black;
}

#content div.static-infos,
#content div.static-infos table td,
#content div.pmMoreInfo,
#content div.pmMoreInfo table td {
    padding-top: 0.5em;
    line-height: 1.25em;
}
#content div.static-infos table th,
#content div.pmMoreInfo table th {
    background: none;
}
div.static-infos table td,
div.pmMoreInfo table td {
   vertical-align: inherit;
}

/* no margin under table in content */
#content div[id^='hook_'] table,
body.cke_editable table,
#content div.static-infos table,
#content div.pmMoreInfo table {
   margin-bottom: 0em !important;
}

/* margin-top when inline-editing */
div[id^='hook_'] > img {
   margin-top: 0.5em;
}

#content table.no-style-table.advices {
    margin-bottom: 0;
}

#content table.no-style-table.advices tr {
    border-left: 0px transparent;
}
/* avoid background of parent table (list of items) being used for advices popup table */
#content table.no-style-table.advices tr td {
    background-color: transparent;
}

table.table-view-widgets {
   width: 100%;
}

/* reduce space between sentence 'query result show only items you can see' and the navigation widget on items listings */
#content-core > div#queryResult {
    margin-top: -1.1em;
}
/* as space is reduced here above, we need to move sentence 'no search results' down */
