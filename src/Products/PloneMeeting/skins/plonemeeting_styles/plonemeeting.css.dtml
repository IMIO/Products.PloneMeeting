/* Colors on states */
.state-active, .label-state-inactive {color: green !important;}
.state-inactive, .state-inactive a, .label-state-inactive {
    color: red !important;
}
.state-created, .label-state-created,
.state-itemcreated, .label-state-itemcreated,
.state-itemcreated_waiting_advices, .label-state-itemcreated_waiting_advices,
.state-advice_under_edit, .label-state-advice_under_edit {
    color: red !important;
}
.state-proposed,
.label-state-proposed,
.state-proposed_waiting_advices,
.label-state-proposed_waiting_advices,
.state-prevalidated,
.label-state-prevalidated,
.state-prevalidated_waiting_advices,
.label-state-prevalidated_waiting_advices,
.state-proposed__or__prevalidated_waiting_advices,
.label-state-proposed__or__prevalidated_waiting_advices,
.state-waiting_advices,
.label-state-waiting_advices,
.state-returned_to_proposing_group_proposed,
.label-state-returned_to_proposing_group_proposed,
.state-returned_to_proposing_group_prevalidated,
.label-state-returned_to_proposing_group_prevalidated {
    color: DarkOrange !important;
}
.state-validated, .label-state-validated { color: blue !important; }
.state-presented, .label-state-presented { color: brown !important; }
.state-itemfrozen, .label-state-itemfrozen,
.state-frozen, .label-state-frozen { color: cornflowerBlue !important; }
.state-returned_to_proposing_group,
.label-state-returned_to_proposing_group,
.state-advice_given,
.label-state-advice_given { color: grey !important; }
.state-itempublished, .label-state-itempublished,
.state-published, .label-state-published { color: darkgoldenrod !important; }
.state-decided, .label-state-decided,
.state-decisions_published, .label-state-decisions_published { color: black !important; }
.state-accepted, .label-state-accepted,
.state-accepted_out_of_meeting, .label-state-accepted_out_of_meeting,
.state-accepted_out_of_meeting_emergency, .label-state-accepted_out_of_meeting_emergency,
.state-confirmed, .label-state-confirmed { color: green !important; }
.state-delayed, .label-state-delayed,
.state-marked_not_applicable, .label-state-marked_not_applicable,
.state-postponed_next_meeting, .label-state-postponed_next_meeting,
.state-removed, .label-state-removed {
    color: grey !important; }
.state-refused, .label-state-refused { color: violet !important; }
.state-closed, .label-state-closed { color: grey !important; }

/* Colors on transitions, used in the item wf history */
/* this correspond to the element creation */
.transition-none { color: red !important; }
.transition-activate {}
.transition-deactivate {color: red !important; }
.transition-propose { color: #ff6c00 !important; }
.transition-validate { color: blue !important; }
.transition-present { color: brown !important; }
.transition-itemfreeze,
.transition-freeze { color: cornFlowerBlue !important; }
.transition-return_to_meeting,
.transition-return_to_proposing_group { color: grey !important; }
.transition-itempublish, .transition-publish { color: darkgoldenrod !important; }
.transition-accept { color: #64c168 !important; }
.transition-confirm { color: green !important; }
.transition-delay { color: grey !important; }
.transition-refuse { color: violet !important; }
.transition-close { color: black !important; }

