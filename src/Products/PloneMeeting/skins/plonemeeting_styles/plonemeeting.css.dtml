/* this avoid losing toolbar when maximizing a CKeditor with very long content */
.cke_inner.cke_reset.cke_maximized {
    position: fixed !important;
}

/* hide collective.documentviewer message on various portaltypes
   that says that element is still not converted. Only hide it on the view
   so error messages are displayed on the edit form. */
body.template-documentviewer.portaltype-configurablepodtemplate dl.portalMessage,
body.template-documentviewer.portaltype-contentcategory dl.portalMessage,
body.template-documentviewer.portaltype-dashboardpodtemplate dl.portalMessage,
body.template-documentviewer.portaltype-file dl.portalMessage,
body.template-documentviewer.portaltype-itemannexcontentcategory dl.portalMessage,
body.template-documentviewer.portaltype-styletemplate dl.portalMessage {
   display: none;
}

/* fix width of select2 widget to 400px as when calling initializeSelect2Widgets
 * because when validation fails, the opened widget in fullscreen (out of the overlay)
 * is shrinked */
.select2-container {
    width: 400px;
}

/* labels displayed on the item view */
