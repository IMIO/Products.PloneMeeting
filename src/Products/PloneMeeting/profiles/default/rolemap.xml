<?xml version="1.0"?>
<rolemap>
  <roles>
    <role name="MeetingArchiveObserver"/>
    <role name="MeetingBudgetImpactEditor"/>
    <role name="MeetingManager"/>
    <role name="MeetingMember"/>
    <role name="MeetingObserverGlobal"/>
    <role name="MeetingObserverLocal"/>
    <role name="MeetingObserverUnpublished"/>
    <role name="MeetingReviewer"/>
  </roles>
  <permissions>
    <permission name="CMFEditions: Save new version"
                acquire="False">
      <role name="Manager"/>
      <role name="Member"/>
    </permission>
    <permission name="CMFEditions: Revert to previous versions"
                acquire="False">
      <role name="Manager"/>
      <role name="Member"/>
    </permission>
    <permission name="CMFEditions: Access previous versions"
                acquire="False">
      <role name="Manager"/>
      <role name="Member"/>
    </permission>
    <permission name="Modify view template"
                acquire="False">
      <role name="Manager"/>
    </permission>
    <permission name="WebDAV access"
                acquire="False">
      <role name="MeetingArchiveObserver"/>
      <role name="MeetingObserverGlobal"/>
      <role name="Manager"/>
    </permission>
    <permission name="Add portal member"
                acquire="False">
      <role name="Manager"/>
    </permission>
  </permissions>
</rolemap>
