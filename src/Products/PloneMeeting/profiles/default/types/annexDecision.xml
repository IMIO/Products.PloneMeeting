 <property name="title" i18n:translate="">Annex decision</property>
 <property name="description" i18n:translate="">Annex decision</property>
 <property name="icon_expr">string:${portal_url}/++resource++dmsmainfile_icon.png</property>
 <property name="factory">annex</property>
