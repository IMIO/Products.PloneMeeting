  <fieldset class="inserting-methods-help-msg">
    <legend i18n:translate="meeting_inserting_methods_help_msg_descr">Parameters of configuration influencing orders of insertion of item in the meeting</legend>
    <br />

    <tal:field define="field python:cfg.getField('insertingMethodsOnAddItem')">
      <span metal:use-macro="context/widgets/field/macros/view" />
    </tal:field>
    <br />
  
    <tal:field condition="view/showSelectablePrivaciesField" define="field python:cfg.getField('selectablePrivacies')">
      <span metal:use-macro="context/widgets/field/macros/view" />
    </tal:field>

    <br />
  </fieldset>

</div>
